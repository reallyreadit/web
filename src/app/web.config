<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="iisnode" path="*" verb="*" modules="iisnode" />
    </handlers>
	 <rewrite>
      <rules>
        <rule name="main">
          <match url="*" />
          <action type="Rewrite" url="server/app/server/main.js" />
        </rule>
      </rules>
    </rewrite> 
  </system.webServer>
</configuration>