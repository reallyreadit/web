{
	"compilerOptions": {
		"downlevelIteration": true,
		"jsx": "react",
		"target": "ES2017",
		"moduleResolution": "node",
		"module": "commonjs",
		"forceConsistentCasingInFileNames": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noUnusedLocals": true
	}
}
