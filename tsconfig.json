{
	"compilerOptions": {
		"jsx": "react",
		"target": "ES5",
		"lib": [
			"dom",
			"es2016",
			"dom.iterable"
		],
		"moduleResolution": "node",
		"module": "commonjs",
		"forceConsistentCasingInFileNames": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noUnusedLocals": true
	}
}